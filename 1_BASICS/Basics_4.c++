// Arithmetic Operators

#include<iostream>
using namespace std;
int main()
{
    int x=5;
    int y=2;
    cout<<x+y<<endl;
    cout<<x-y<<endl;
    cout<<x*y<<endl<<endl;

    cout<<x/y;  // 5/2=2.5 but o/p is 2 b/c int pe koi bhi operation int hi return krega --> basically quotient return krr rha hai
}
